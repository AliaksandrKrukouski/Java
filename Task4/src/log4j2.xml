<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<properties>
		<property name="name">multithreading</property>
		<property name="pattern">%d{yyyy-MM-dd HH:mm:ss} [%t] - %msg%n</property>
	</properties>
	<appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="${pattern}"/>
		</Console>
	</appenders>
	<loggers>
		<root level="debug">
			<appender-ref ref="Console"/>
		</root>
	</loggers>
</configuration>